{
  "name": "testistic-model",
  "version": "1.0.5",
  "author": "Phil <phil@xceptionale.com>",
  "license": "MIT",
  "description": "Testistic Model functionality",
  "main": "dist/lib.umd.js",
  "unpkg": "dist/lib.umd.js",
  "file": [ "dist" ],
  "keywords": [
    "testistic"
  ],
  "scripts": {
    "prebuild": "npm run lint",
    "lint": "eslint src",
    "lint:tests": "eslint tests",
    "build": "rollup -c",
    "start": "npm run test:watch",
    "test:watch": "nodemon --watch src --watch tests --exec 'npm test && npm run lint && npm run build'",
    "test": "mocha -r reify -r source-map-support/register tests/*.spec.js",
    "prepublish": "npm run build",
    "test:browser": "npm run build && opn http://127.0.0.1:8080/tests/ && serve -p 8080"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.0.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "opn-cli": "^3.1.0",
    "reify": "^0.16.2",
    "rollup": "^0.61.2",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "serve": "^9.1.0",
    "source-map-support": "^0.5.6"
  },
  "dependencies": {
    "uuid": "^3.3.2"
  }
}
